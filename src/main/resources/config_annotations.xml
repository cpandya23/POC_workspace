<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-3.1.xsd ">
<context:annotation-config></context:annotation-config>

	

	<bean id="employeeBean" class="mypack_Annotations.Employee">
	<!-- 	<constructor-arg value="CKP"></constructor-arg>
		<constructor-arg value="ABC"></constructor-arg>
	 -->
		<property name="ename" value="CKP"></property>
		<property name="role" value="CKP"></property>
		
	</bean>

		

	<bean id="address" class="mypack_Annotations.Address">
		<constructor-arg value="BRC"></constructor-arg>
		<constructor-arg value="123"></constructor-arg>

	</bean>

</beans>