<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-3.1.xsd ">
<context:annotation-config></context:annotation-config>

	
	
	<bean id="employee" class="mypack_Exercise.Employee">

	
		
	</bean>

		

	<bean id="officeAddress" class="mypack_Exercise.Address">
		<constructor-arg value="1"></constructor-arg>
		<constructor-arg value="INDIA"></constructor-arg>
		<constructor-arg value="BVN"></constructor-arg>
		<constructor-arg value="KBD"></constructor-arg>
		<constructor-arg value="411057"></constructor-arg>
	</bean>
	
	<bean id="residentAddress" class="mypack_Exercise.Address">
		<constructor-arg value="2"></constructor-arg>
		<constructor-arg value="INDIA"></constructor-arg>
		<constructor-arg value="BVNH"></constructor-arg>
		<constructor-arg value="KBDH"></constructor-arg>
		<constructor-arg value="411057"></constructor-arg>
	</bean>
	


		

	
</beans>